{
    "name": "saberqadimi/link-shortener",
    "description": "A Laravel package for shortening links.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "link shortener",
        "url shortener",
        "short url",
        "shorten link",
        "php package"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\LinkShortener\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Qadimi",
            "email": "qadimi74@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\LinkShortener\\LinkShortenerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
